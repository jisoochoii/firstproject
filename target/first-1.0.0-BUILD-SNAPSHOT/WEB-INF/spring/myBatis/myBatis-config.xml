        <typeAlias type="com.whizlab.first.vo.UserVO" alias="UserVO"/>
        <typeAlias type="com.whizlab.first.vo.BrandVO" alias="BrandVO"/>
        <typeAlias type="com.whizlab.first.vo.GroupVO" alias="GroupVO"/>
        <typeAlias type="com.whizlab.first.vo.ScreenVO" alias="ScreenVO"/>
        <typeAlias type="com.whizlab.first.vo.CenterVO" alias="CenterVO"/>
        <typeAlias type="com.whizlab.first.vo.CourierVO" alias="CourierVO"/>
        <typeAlias type="com.whizlab.first.vo.ProductVO" alias="ProductVO"/>
        <typeAlias type="com.whizlab.first.vo.CustomerVO" alias="CustomerVO"/>
        <typeAlias type="com.whizlab.first.vo.CommonMaxVO" alias="CommonMaxVO"/>
        <typeAlias type="com.whizlab.first.vo.FreeboardVO" alias="FreeboardVO"/>
        <typeAlias type="com.whizlab.first.vo.ThumbnailVO" alias="ThumbnailVO"/>
        <typeAlias type="com.whizlab.first.vo.UserGroupVO" alias="UserGroupVO"/>
        <typeAlias type="com.whizlab.first.vo.CommonCodeVO" alias="CommonCodeVO"/>
        <typeAlias type="com.whizlab.first.vo.MainCommonVO" alias="MainCommonVO"/>
        <typeAlias type="com.whizlab.first.vo.GroupScreenVO" alias="GroupScreenVO"/>
<!--        <typeAlias type="java.lang.String" alias="String"/>-->
